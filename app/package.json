{
  "name": "fullstack-template-app",
  "version": "1.0.0",
  "description": "Full-stack Next.js application with Supabase and Redis",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "npm run test:e2e:docker",
    "test:a11y": "vitest run --config vitest.a11y.config.ts",
    "test:quick": "npm run test:a11y",
    "test:integration": "npm run test:docker",
    "test:all": "npm run test:docker && npm run test:e2e:docker",
    "test:docker": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml -f docker-compose.test.yml up --build --abort-on-container-exit test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:docker": "cd .. && docker-compose -f docker-compose.e2e.yml up --build --abort-on-container-exit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "supabase:start": "cd .. && supabase start",
    "supabase:stop": "cd .. && supabase stop",
    "supabase:status": "cd .. && supabase status",
    "supabase:reset": "cd .. && supabase db reset"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.79",
    "@ai-sdk/react": "^2.0.109",
    "@measured/puck": "^0.20.2",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.38.0",
    "ai": "^5.0.108",
    "iron-session": "^8.0.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.0",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@chromatic-com/storybook": "^4.1.3",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.3",
    "@storybook/addon-docs": "^10.1.3",
    "@storybook/addon-onboarding": "^10.1.3",
    "@storybook/addon-vitest": "^10.1.3",
    "@storybook/nextjs-vite": "^10.1.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.11.0",
    "axe-playwright": "^2.2.2",
    "dotenv": "^16.6.1",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-storybook": "^10.1.3",
    "happy-dom": "^20.0.10",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.2.0",
    "msw": "^2.12.2",
    "playwright": "^1.57.0",
    "postcss": "^8.4.0",
    "storybook": "^10.1.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^7.2.6",
    "vitest": "^4.0.13"
  }
}

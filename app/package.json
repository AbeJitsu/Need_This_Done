{
  "name": "fullstack-template-app",
  "version": "1.0.0",
  "description": "Full-stack Next.js application with Supabase and Redis",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "npm run test:e2e",
    "test:a11y": "vitest run --config vitest.a11y.config.ts",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:a11y:e2e": "playwright test e2e/accessibility.a11y.test.ts --project=e2e-bypass",
    "test:a11y:full": "npm run test:a11y && npm run test:a11y:e2e",
    "test:quick": "npm run test:unit && npm run test:a11y",
    "test:all": "npm run test:unit && npm run test:a11y && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "supabase:start": "cd .. && supabase start",
    "supabase:stop": "cd .. && supabase stop",
    "supabase:status": "cd .. && supabase status",
    "supabase:reset": "cd .. && supabase db reset",
    "test:emails": "npx tsx scripts/test-emails.ts",
    "screenshot:map": "npx tsx scripts/build-component-map.ts",
    "screenshot:affected": "npx tsx scripts/screenshot-affected.ts",
    "changelog:complete": "npx tsx scripts/complete-changelog.ts",
    "cleanup:e2e": "npx tsx scripts/cleanup-e2e-posts.ts",
    "generate:manifest": "npx tsx scripts/generate-content-manifest.ts",
    "prebuild": "npm run generate:manifest"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.79",
    "@ai-sdk/react": "^2.0.109",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@react-email/components": "^0.0.15",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tiptap/extension-color": "^3.14.0",
    "@tiptap/extension-image": "^3.14.0",
    "@tiptap/extension-link": "^3.14.0",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/extension-text-align": "^3.14.0",
    "@tiptap/extension-text-style": "^3.14.0",
    "@tiptap/extension-underline": "^3.14.0",
    "@tiptap/pm": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "ai": "^5.0.108",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.26.2",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.18.0",
    "highlight.js": "^11.11.1",
    "iron-session": "^8.0.0",
    "lucide-react": "^0.562.0",
    "next": "^14.2.35",
    "next-auth": "^4.24.13",
    "re-resizable": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "redis": "^4.6.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "resend": "^3.0.0",
    "stripe": "^20.0.0",
    "svix": "^1.82.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@chromatic-com/storybook": "^4.1.3",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.3",
    "@storybook/addon-docs": "^10.1.3",
    "@storybook/addon-onboarding": "^10.1.3",
    "@storybook/addon-vitest": "^10.1.3",
    "@storybook/nextjs-vite": "^10.1.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.11.0",
    "axe-playwright": "^2.2.2",
    "dotenv": "^16.6.1",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "happy-dom": "^20.0.10",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.2.0",
    "msw": "^2.12.2",
    "playwright": "^1.57.0",
    "postcss": "^8.4.0",
    "sharp": "^0.34.5",
    "storybook": "^10.1.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^7.2.6",
    "vitest": "^4.0.13"
  }
}

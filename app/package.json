{
  "name": "fullstack-template-app",
  "version": "1.0.0",
  "description": "Full-stack Next.js application with Supabase and Redis",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "vitest run __tests__/components/DatabaseDemo.a11y.test.tsx __tests__/components/AuthDemo.a11y.test.tsx __tests__/components/TechnicalFlow.a11y.test.tsx",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:integration:run": "vitest run --config vitest.integration.config.ts",
    "test:all": "npm run test:run && npm run test:integration:run",
    "test:docker": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml -f docker-compose.test.yml up --build --abort-on-container-exit test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "iron-session": "^8.0.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.11.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "happy-dom": "^20.0.10",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.2.0",
    "msw": "^2.12.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.13"
  }
}
